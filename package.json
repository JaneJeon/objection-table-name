{
  "name": "objection-table-name",
  "description": "ActiveRecord-style objection.js plugin to auto-fill tableName",
  "version": "1.2.1",
  "main": "index.js",
  "repository": "JaneJeon/objection-table-name",
  "author": "Jane Jeon <JaneJeon9719@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --ci --reporters=default --reporters=jest-junit",
    "lint": "standard | snazzy && prettier --check '**/*.{md,json,yml,yaml}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "standard --fix",
      "pre-push": "yarn test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.{md,json,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.memoize": "^4.1.2",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "husky": "^4.0.7",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "knex": "^0.21.0",
    "lint-staged": "^10.0.0",
    "objection-1": "npm:objection@1",
    "objection-2": "npm:objection@2",
    "prettier": "^1.18.2",
    "snazzy": "^8.0.0",
    "standard": "^14.1.0"
  },
  "peerDependencies": {
    "objection": "^1 || ^2"
  },
  "keywords": [
    "objection",
    "objection.js",
    "class",
    "tablename",
    "activerecord",
    "plugin"
  ]
}
